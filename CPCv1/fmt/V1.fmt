	partsfont      Helvetica-Bold 12
	subtitlefont   Times-Roman    12
	textfont       Times-Roman    11
	titlefont      Helvetica-Bold 16
	vocalfont      Helvetica-Bold 14
	wordsfont      Helvetica-Bold 12

	landscape     0
	lineskipfac   1.1
	maxshrink     1.0
	stretchlast   0
